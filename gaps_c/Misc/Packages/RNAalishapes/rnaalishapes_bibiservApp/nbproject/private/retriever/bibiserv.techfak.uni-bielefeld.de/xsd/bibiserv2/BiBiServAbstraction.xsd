    <xs:simpleType name="TjsfElement">
        <xs:annotation>
            <xs:documentation xml:lang="en"> TjsfElement contain all usefull elements to create an
            .autogenerated tool submission page. </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="selectOneRadio"/>
            <xs:enumeration value="selectOneMenu"/>
            <xs:enumeration value="selectManyMenu"/>
            <xs:enumeration value="selectOneListbox"/>
            <xs:enumeration value="selectManyListbox"/>
            <xs:enumeration value="selectManyCheckbox"/>
            <xs:enumeration value="selectBooleanCheckbox"/>
            <xs:enumeration value="inputText"/>
            <xs:enumeration value="inputTextarea"/>
            <xs:enumeration value="inputHidden"/>
            <xs:enumeration value="inputSecret"/>
        </xs:restriction>
    </xs:simpleType>
    
    
    <xs:complexType name="Twebstart">
        <xs:annotation>
            <xs:documentation>Twebstart is a special view for webstart application. In addition to all
                elements a normal "view" contains it provides some required elements to describe the webstart
                application. These "informations" are used by BiBiServWebstartServlet to generate a  suitable
                jnlp file.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="title" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The 'title' element is used as user-visible screen name(s) 
                        for this webstart element, possibly localized to a specific language by using the "lang" 
                        attribute. </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute ref="xml:lang" use="optional" default="en">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en"> The 'lang' attribute defines
                                        the natural language name of the content of this element.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="introductoryText" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The 'introductoryText' sub-element(s) of a
                        webstart element contains an introducton to the webstart page (of
                        course possibly localized to a specific language by using the "lang"
                        attribute) and is placed BEFORE the auto-generated webstart link. 
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="minihtml:Flow">
                            <xs:attribute ref="xml:lang" use="optional" default="en">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en"> The 'lang' attribute defines
                                        the natural language name of the content of this element.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="customContent" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The 'customContent' sub-element(s) of a webstart
                        element contain marked-up manual quality user information text, possibly
                        localized to a specific language by using the "lang" attribute. This will
                        generally appear AFTTER the auto-generated webstart link.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:complexContent>
                        <xs:extension base="minihtml:Flow">
                            <xs:attribute ref="xml:lang" use="optional" default="en">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en"> The 'lang' attribute defines
                                        the natural language name of the content of this element.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:complexContent>
                </xs:complexType>
            </xs:element>
            <xs:element ref="jnlp:jnlp"/>
        </xs:sequence>
        <xs:attribute name="id" type="xs:ID" use="required">
            <xs:annotation>
                <xs:documentation> Manuals REQUIRE an ID to allow direct navigation. Convention for
                    the id is: ToolID+ "_" + "manual" </xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>
    
</xs:schema>
