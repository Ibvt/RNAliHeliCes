    <xs:simpleType name="TexecutableTypes">
        <xs:annotation>
            <xs:documentation xml:lang="en"> The 'TexecutableTypes' type contains all currently
                supported kinds of executable server resources, which might have to be treated
            differently in call generation. </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="javaclass"/>
            <xs:enumeration value="binary"/>
            <xs:enumeration value="perlScript"/>
        </xs:restriction>
    </xs:simpleType>


    <xs:complexType name="Texample">
        <xs:annotation>
            <xs:documentation xml:lang="en">
                Texample contains an example input/parameter set. The description is mandatory. 
                An example is set using the prop tag, the attributes points to the corresponding 
                input/param/enumparam and text content the value. Each function can have more 
                than one example.
            </xs:documentation>
        </xs:annotation>  
        <xs:sequence>
            <xs:element name="name" minOccurs="1" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Name of example. At least one name must be given.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute ref="xml:lang" use="optional" default="en">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en"> The 'lang' attribute defines
                                        the natural language name of the content of this element.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="description" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>Description of example.</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="xs:string">
                            <xs:attribute ref="xml:lang" use="optional" default="en">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en"> The 'lang' attribute defines
                                        the natural language name of the content of this element.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="prop" minOccurs="0" maxOccurs="unbounded">
                <xs:annotation>
                    <xs:documentation>
                        Defines an example value that binds to an input or (enum)param using attribute idref. 
                        Values for input must be a base64 encoded string, values for (enum)param  are strings.
                    </xs:documentation>
                </xs:annotation>
                <xs:complexType>
                    <xs:simpleContent >
                        <xs:extension base="xs:string">
                            <xs:attribute name="idref" type="xs:string" use="required">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">
                                        Must be a valid reference to an id of the corresponding 
                                        input/param/enumparam.
                                    </xs:documentation>
                                </xs:annotation>
                            </xs:attribute>
                        </xs:extension>                  
                    </xs:simpleContent>                
                </xs:complexType>
            </xs:element>     
        </xs:sequence>
    </xs:complexType>

