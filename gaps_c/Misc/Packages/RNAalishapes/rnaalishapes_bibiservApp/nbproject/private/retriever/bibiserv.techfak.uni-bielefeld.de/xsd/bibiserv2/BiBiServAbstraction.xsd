    <xs:simpleType name="Tprimitive">
        <xs:annotation>
            <xs:documentation xml:lang="en"> These are the allowed types of tool Parameters. They
                have been chosen because they are a least common denominator of data types in
                different programming paradigms and languages, and are directly equivalent to the
                BioMOBY primitive data types. There has to be some rather clever parsing code to
                match these strings against the REAL types of values of parameters which are defined
            by this! </xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A Boolean (truth) value
                    (true/false)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="int">
                <xs:annotation>
                    <xs:documentation xml:lang="en">An Integer value</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="float">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A floating point number value
                    (true/false)</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="string">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A string of characters</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="datetime">
                <xs:annotation>
                    <xs:documentation xml:lang="en">A datetime string according to ISO
                    8601</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
